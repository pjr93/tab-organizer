{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.0",
  "description": "Collects tabs and windows and allows a 2D environment for regrouping.",
  "permissions": [
    "tabs",
    "windows",
    "storage",
    "nativeMessaging",
    "activeTab",
    "favicon"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "commands": {
    "home": {
      "suggested_key": {
        "default": "Alt+Shift+Home",
        "mac": "Alt+Shift+Home"
      },
      "description": "Goes to the next tab in the visited list"
    },
    "next_tab": {
      "suggested_key": {
        "default": "Alt+Shift+Right",
        "mac": "Alt+Shift+Right"
      },
      "description": "Goes to the next tab in the visited list"
    },
    "prev_tab": {
      "suggested_key": {
        "default": "Alt+Shift+Left",
        "mac": "Alt+Shift+Left"
      },
      "description": "Goes to the next tab in the visited list"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}